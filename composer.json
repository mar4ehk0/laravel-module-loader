        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mar4ehk0\\ModuleLoader\\ModuleLoaderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
